package cepneuralink;

import sbnz.mrsandman.neuralinkapp.model.User;


rule "Detect if the user is static"
    when
        $u: User(speed == 0 || speed >= 10)
    then
    	System.out.println("Aktivacija pravila za detektovanje staticnosti.");
        modify($u) {
        	setIsStatic(true);
        }
end